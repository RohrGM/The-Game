[32m+[m[32mextends Node2D[m
[32m+[m
[32m+[m[32monready var nav : Navigation2D = get_tree().get_root().get_node("World")[m
[32m+[m[32monready var map = get_tree().get_root().get_node("World/BattleMap")[m
[32m+[m
[32m+[m[32mvar speed : int = 5[m
[32m+[m
[32m+[m[32mvar end_pos = Vector2()[m
[32m+[m[32mvar path = [][m
[32m+[m
[32m+[m[32msignal in_position()[m
[32m+[m
[32m+[m[32mfunc _ready() -> void:[m
[32m+[m	[32mset_physics_process(false)[m
[32m+[m[41m	[m
[32m+[m[32mfunc _physics_process(delta : float) -> void:[m
[32m+[m	[32mif path.size() > 0:[m
[32m+[m		[32mmove_along_path(delta * speed)[m
[32m+[m	[32melse:[m
[32m+[m		[32memit_signal("in_position")[m
[32m+[m		[32mset_physics_process(false)[m
[32m+[m[41m		[m
[32m+[m[32mfunc start(pos: Vector2, speed_move : int = 30, type : String = "Run") -> void:[m
[32m+[m	[32mspeed = speed_move[m
[32m+[m	[32mend_pos = pos[m
[32m+[m	[32mget_parent().travel_anim(type)[m
[32m+[m	[32m_update_navigation_path()[m
[32m+[m
[32m+[m	[32mset_physics_process(true)[m
[32m+[m[41m	[m
[32m+[m[32mfunc _update_navigation_path() -> void:[m
[32m+[m	[32mpath = nav.get_simple_path(get_parent().position, end_pos, true)[m
[32m+[m	[32mpath.remove(0)[m
[32m+[m
[32m+[m[32mfunc move_along_path(distance : float) -> void:[m
[32m+[m[41m	[m
[32m+[m	[32mvar last_point : Vector2 = get_parent().position[m
[32m+[m	[32mvar input_vector : Vector2 = Vector2.ZERO[m
[32m+[m[41m	[m
[32m+[m	[32minput_vector.x = path[0].x - last_point.x[m
[32m+[m	[32minput_vector.y = path[0].y - last_point.y[m
[32m+[m[41m	[m
[32m+[m	[32mget_parent().update_anim_tree(input_vector.normalized())[m
[32m+[m	[32mwhile path.size():[m
[32m+[m		[32mvar distance_between_points = last_point.distance_to(path[0])[m
[32m+[m
[32m+[m		[32mif distance <= distance_between_points:[m
[32m+[m			[32mget_parent().position = last_point.linear_interpolate(path[0], distance / distance_between_points)[m
[32m+[m			[32mreturn[m
[32m+[m		[32mdistance -= distance_between_points[m
[32m+[m		[32mlast_point = path[0][m
[32m+[m		[32mpath.remove(0)[m
[32m+[m
[32m+[m	[32mget_parent().position = last_point[m
[32m+[m	[32mset_physics_process(false)[m
[32m+[m	[32memit_signal("in_position")[m
