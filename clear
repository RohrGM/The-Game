[32m+[m[32mextends Base[m
[32m+[m[32mclass_name EnemyBase[m
[32m+[m
[32m+[m[32menum mode{[m
[32m+[m	[32mBLOCKED,[m
[32m+[m	[32mFREE,[m
[32m+[m	[32mTURN[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvar target = null[m
[32m+[m[32mvar current_mode = mode.FREE[m
[32m+[m[32mvar life = 10[m
[32m+[m
[32m+[m[32mfunc set_mode(md):[m
[32m+[m	[32mcurrent_mode = md[m
[32m+[m	[32mmatch current_mode:[m
[32m+[m		[32mmode.BLOCKED:[m
[32m+[m			[32mposition = center_block(position)[m
[32m+[m		[32mmode.TURN:[m
[32m+[m			[32mattributes.action_points = attributes.dex[m
[32m+[m			[32mif attack_range(get_enemy().position):[m
[32m+[m				[32mattack()[m
[32m+[m				[32mprint("alcance")[m
[32m+[m			[32melse:[m
[32m+[m				[32mprint("fora de alcance")[m
[32m+[m				[32mvar pos = move_in_battle(get_enemy().position)[m
[32m+[m				[32mif pos != null:[m
[32m+[m					[32mattributes.action_points -= pos_in_range(center_block(pos), attributes.action_points)[m
[32m+[m					[32mmove_at(center_block(pos), "Run")[m
[32m+[m[41m		[m
[32m+[m[32mfunc _on_MoveAt_in_position():[m
[32m+[m	[32mtravel_anim("Idle")[m
[32m+[m	[32mmatch current_mode:[m
[32m+[m		[32mmode.TURN:[m
[32m+[m			[32mif attributes.action_points > 0 and attack_range(get_enemy().position):[m
[32m+[m				[32mattack()[m
[32m+[m[41m				[m
[32m+[m[32m#func get_distance(pos):[m
[32m+[m[32m#	var nav = get_tree().get_root().get_node("World")[m
[32m+[m[32m#	var path = nav.get_simple_path(position, pos, false)[m
