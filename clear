[32m+[m[32mextends TextureRect[m
[32m+[m
[32m+[m[32mconst item_base = preload("res://PackageScenes/ItemBase.tscn")[m
[32m+[m
[32m+[m[32mvar items = [][m
[32m+[m[32mvar cells = [][m
[32m+[m[41m [m
[32m+[m[32mvar grid = {}[m
[32m+[m[32mvar cell_size = 8[m
[32m+[m[32mvar grid_width = 0[m
[32m+[m[32mvar grid_height = 0[m
[32m+[m[41m [m
[32m+[m[32mfunc _ready():[m
[32m+[m	[32mvar s = get_grid_size(self)[m
[32m+[m	[32mgrid_width = s.x[m
[32m+[m	[32mgrid_height = s.y[m[41m [m
[32m+[m	[32mfor x in range(grid_width):[m
[32m+[m		[32mgrid[x] = {}[m
[32m+[m		[32mfor y in range(grid_height):[m
[32m+[m			[32m$TileMap.set_cellv(Vector2(x,y), 0)[m
[32m+[m			[32mgrid[x][y] = false[m
[32m+[m
[32m+[m[32mfunc find_item(item):[m
[32m+[m	[32mfor i in items:[m
[32m+[m		[32mif item == i.get_meta("id"):[m
[32m+[m			[32mreturn true[m
[32m+[m	[32mreturn false[m
[32m+[m[41m [m
[32m+[m[32mfunc insert_item(item):[m
[32m+[m	[32mvar item_pos = item.rect_global_position + Vector2(cell_size / 2, cell_size / 2)[m
[32m+[m	[32mvar g_pos = pos_to_grid_coord(item_pos)[m
[32m+[m	[32mvar item_size = get_grid_size(item)[m
[32m+[m	[32mif is_grid_space_available(g_pos.x, g_pos.y, item_size.x, item_size.y):[m
[32m+[m		[32mset_grid_space(g_pos.x, g_pos.y, item_size.x, item_size.y, true)[m
[32m+[m		[32mitem.rect_global_position = rect_global_position + Vector2(g_pos.x, g_pos.y) * cell_size[m
[32m+[m		[32mitems.append(item)[m
[32m+[m		[32mreturn true[m
[32m+[m	[32melse:[m
[32m+[m		[32mreturn false[m
[32m+[m[41m		[m
[32m+[m[32mfunc show_cells(item):[m
[32m+[m	[32mvar item_pos = item.rect_global_position + Vector2(cell_size / 2, cell_size / 2)[m
[32m+[m	[32mvar g_pos = pos_to_grid_coord(item_pos)[m
[32m+[m	[32mvar item_size = get_grid_size(item)[m
[32m+[m	[32mset_cell_space(g_pos.x, g_pos.y, item_size.x, item_size.y)[m
[32m+[m[41m	[m
[32m+[m[32mfunc hide_cells():[m
[32m+[m	[32mfor i in cells:[m
[32m+[m		[32mif $TileMap.get_cellv(i) == 3:[m
[32m+[m			[32m$TileMap.set_cellv(i, 1)[m
[32m+[m		[32melif $TileMap.get_cellv(i) == 2:[m
[32m+[m			[32m$TileMap.set_cellv(i, 0)[m
[32m+[m	[32mcells.clear()[m
[32m+[m[41m [m
[32m+[m[32mfunc grab_item(pos):[m
[32m+[m	[32mvar item = get_item_under_pos(pos)[m
[32m+[m	[32mif item == null:[m
[32m+[m		[32mreturn null[m
[32m+[m[41m   [m
[32m+[m	[32mvar item_pos = item.rect_global_position + Vector2(cell_size / 2, cell_size / 2)[m
[32m+[m	[32mvar g_pos = pos_to_grid_coord(item_pos)[m
[32m+[m	[32mvar item_size = get_grid_size(item)[m
[32m+[m	[32mset_grid_space(g_pos.x, g_pos.y, item_size.x, item_size.y, false)[m
[32m+[m[41m   [m
[32m+[m	[32mitems.remove(items.find(item))[m
[32m+[m	[32mreturn item[m
[32m+[m[41m [m
[32m+[m[32mfunc pos_to_grid_coord(pos):[m
[32m+[m	[32mvar local_pos = pos - rect_global_position[m
[32m+[m	[32mvar results = {}[m
[32m+[m	[32mresults.x = int(local_pos.x / cell_size)[m
[32m+[m	[32mresults.y = int(local_pos.y / cell_size)[m
[32m+[m	[32mreturn results[m
[32m+[m[41m [m
[32m+[m[32mfunc get_grid_size(item):[m
[32m+[m	[32mvar results = {}[m
[32m+[m	[32mvar s = item.rect_size[m
[32m+[m	[32mresults.x = clamp(int(s.x / cell_size), 1, 500)[m
[32m+[m	[32mresults.y = clamp(int(s.y / cell_size), 1, 500)[m
[32m+[m	[32mreturn results[m
[32m+[m[41m [m
[32m+[m[32mfunc is_grid_space_available(x, y, w ,h):[m
[32m+[m	[32mif x < 0 or y < 0:[m
[32m+[m		[32mreturn false[m
[32m+[m	[32mif x + w > grid_width or y + h > grid_height:[m
[32m+[m		[32mreturn false[m
[32m+[m	[32mfor i in range(x, x + w):[m
[32m+[m		[32mfor j in range(y, y + h):[m
[32m+[m			[32mif grid[i][j]:[m
[32m+[m				[32mreturn false[m
[32m+[m	[32mreturn true[m
[32m+[m[41m	[m
[32m+[m[32mfunc set_cell_space(x, y, w, h):[m
[32m+[m	[32mhide_cells()[m
[32m+[m	[32mfor i in range(x, x + w):[m
[32m+[m		[32mfor j in range(y, y + h):[m
[32m+[m			[32mif $TileMap.get_cellv(Vector2(i,j)) == 1:[m
[32m+[m				[32m$TileMap.set_cellv(Vector2(i,j), 3)[m
[32m+[m				[32mcells.append(Vector2(i,j))[m
[32m+[m			[32melif $TileMap.get_cellv(Vector2(i,j)) == 0:[m
[32m+[m				[32m$TileMap.set_cellv(Vector2(i,j), 2)[m
[32m+[m				[32mcells.append(Vector2(i,j))[m
[32m+[m[41m [m
[32m+[m[32mfunc set_grid_space(x, y, w, h, state):[m
[32m+[m	[32mfor i in range(x, x + w):[m
[32m+[m		[32mfor j in range(y, y + h):[m
[32m+[m			[32mif state:[m
[32m+[m				[32m$TileMap.set_cellv(Vector2(i,j), 1)[m
[32m+[m			[32melse:[m
[32m+[m				[32m$TileMap.set_cellv(Vector2(i,j), 0)[m
[32m+[m			[32mgrid[i][j] = state[m
[32m+[m[41m [m
[32m+[m[32mfunc get_item_under_pos(pos):[m
[32m+[m	[32mfor item in items:[m
[32m+[m		[32mif item.get_global_rect().has_point(pos):[m
[32m+[m			[32mreturn item[m
[32m+[m	[32mreturn null[m
[32m+[m[41m [m
[32m+[m[32mfunc insert_item_at_first_available_spot(item):[m
[32m+[m	[32mfor y in range(grid_height):[m
[32m+[m		[32mfor x in range(grid_width):[m
[32m+[m			[32mif !grid[x][y]:[m
[32m+[m				[32mitem.rect_global_position = rect_global_position + Vector2(x, y) * cell_size[m
[32m+[m				[32mif insert_item(item):[m
[32m+[m					[32mreturn true[m
[32m+[m	[32mreturn false[m
