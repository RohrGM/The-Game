[32m+[m[32mfunc _ready():[m
[32m+[m	[32mpickup_item("Rifle")[m
[32m+[m	[32mpickup_item("Rifle")[m
[32m+[m
[32m+[m[32mfunc _input(event):[m
[32m+[m	[32mvar cursor_pos = get_global_mouse_position()[m
[32m+[m	[32mif event.is_action_pressed("inv_grab"):[m
[32m+[m		[32mgrab(cursor_pos)[m
[32m+[m	[32mif event.is_action_released("inv_grab"):[m
[32m+[m		[32mrelease(cursor_pos)[m
[32m+[m	[32mif item_held != null:[m
[32m+[m		[32m$GridBackPack.show_cells(item_held)[m
[32m+[m		[32m$GridLoot.show_cells(item_held)[m
[32m+[m		[32mitem_held.rect_global_position = cursor_pos + item_offset[m
[32m+[m[41m		[m
[32m+[m[41m [m
[32m+[m[32mfunc grab(cursor_pos):[m
[32m+[m	[32mvar c = get_container_under_cursor(cursor_pos)[m
[32m+[m	[32mif c != null and c.has_method("grab_item"):[m
[32m+[m		[32mitem_held = c.grab_item(cursor_pos)[m
[32m+[m[41m	[m
[32m+[m		[32mif is_instance_valid(item_held):[m
[32m+[m			[32mif c == $GridLoot:[m
[32m+[m				[32mlocal_loot.loot.erase(item_held.get_meta("id"))[m
[32m+[m				[32mprint("removi item do loot")[m
[32m+[m			[32mlast_container = c[m
[32m+[m			[32mlast_pos = item_held.rect_global_position[m
[32m+[m			[32mitem_offset = item_held.rect_global_position - cursor_pos[m
[32m+[m			[32mmove_child(item_held, get_child_count())[m
[32m+[m			[32m$GridBackPack.show_cells(item_held)[m
[32m+[m[41m [m
[32m+[m[32mfunc release(cursor_pos):[m
[32m+[m	[32mif item_held == null:[m
[32m+[m		[32mreturn[m
[32m+[m	[32mvar c = get_container_under_cursor(cursor_pos)[m
[32m+[m	[32mif c == null:[m
[32m+[m		[32mdrop_item()[m
[32m+[m	[32melif c.has_method("insert_item"):[m
[32m+[m
[32m+[m		[32mif c.insert_item(item_held):[m
[32m+[m			[32mif c == $GridLoot:[m
[32m+[m				[32mprint("coloquei item no loot")[m
[32m+[m				[32mlocal_loot.loot.append(item_held.get_meta("id"))[m
[32m+[m			[32mitem_held = null[m
[32m+[m			[32m$GridBackPack.hide_cells()[m
[32m+[m			[32m$GridLoot.hide_cells()[m
[32m+[m		[32melse:[m
[32m+[m			[32m$GridBackPack.hide_cells()[m
[32m+[m			[32m$GridLoot.hide_cells()[m
[32m+[m			[32mreturn_item()[m
[32m+[m	[32melse:[m
[32m+[m		[32m$GridBackPack.hide_cells()[m
[32m+[m		[32m$GridLoot.hide_cells()[m
[32m+[m		[32mreturn_item()[m
[32m+[m[41m   [m
[32m+[m[32mfunc get_container_under_cursor(cursor_pos):[m
[32m+[m	[32mvar containers = [$GridBackPack, $EquipmentSlots, $InventoryBase, $GridLoot, $LootBase][m
[32m+[m	[32mfor c in containers:[m
[32m+[m		[32mif c.get_global_rect().has_point(cursor_pos):[m
[32m+[m			[32mreturn c[m
[32m+[m	[32mreturn null[m
[32m+[m[41m [m
[32m+[m[32mfunc inv_show():[m
[32m+[m	[32mshow()[m
[32m+[m	[32m$GridLoot.hide()[m
[32m+[m[41m	[m
[32m+[m[32mfunc inv_hide():[m
[32m+[m	[32mhide()[m
[32m+[m	[32m$GridLoot.hide()[m
[32m+[m	[32m$GridLoot.clear_items()[m
[32m+[m	[32mlocal_loot = null[m
[32m+[m[41m	[m
[32m+[m[32mfunc inv_loot_show(lt):[m
[32m+[m	[32mshow()[m
[32m+[m	[32madd_loot(lt)[m
[32m+[m	[32m$GridLoot.show()[m
[32m+[m[41m	[m
[32m+[m[32mfunc drop_item():[m
[32m+[m	[32m$GridBackPack.hide_cells()[m
[32m+[m	[32mitem_held.queue_free()[m
[32m+[m	[32mitem_held = null[m
[32m+[m[41m [m
[32m+[m[32mfunc return_item():[m
[32m+[m	[32mitem_held.rect_global_position = last_pos[m
[32m+[m	[32mlast_container.insert_item(item_held)[m
[32m+[m	[32mitem_held = null[m
[32m+[m[41m	[m
[32m+[m[32mfunc add_loot(loot):[m
[32m+[m	[32mlocal_loot = loot[m
[32m+[m	[32mfor item_id in local_loot.loot:[m
[32m+[m		[32mvar item = item_base.instance()[m
[32m+[m		[32mitem.set_meta("id", item_id)[m
[32m+[m		[32mitem.texture = load(ItemDB.get_item(item_id)["icon"])[m
[32m+[m		[32madd_child(item)[m
[32m+[m[41m		[m
[32m+[m		[32mif !$GridLoot.insert_item_at_first_available_spot(item):[m
[32m+[m			[32mitem.queue_free()[m
[32m+[m[41m			[m
[32m+[m		[32melif ItemDB.get_item(item_id).has("type")  and ItemDB.get_item(item_id)["type"] == "QUEST":[m
[32m+[m			[32memit_signal("item_quest_find", item_id)[m
[32m+[m		[32memit_signal("item_add", item_id)[m
[32m+[m[41m	[m
[32m+[m[41m [m
[32m+[m[32mfunc pickup_item(item_id):[m
[32m+[m	[32mvar item = item_base.instance()[m
[32m+[m	[32mitem.set_meta("id", item_id)[m
[32m+[m	[32mitem.texture = load(ItemDB.get_item(item_id)["icon"])[m
[32m+[m	[32madd_child(item)[m
[32m+[m[41m	[m
[32m+[m	[32mif !$GridBackPack.insert_item_at_first_available_spot(item):[m
[32m+[m		[32mitem.queue_free()[m
[32m+[m		[32mreturn false[m
[32m+[m	[32mif ItemDB.get_item(item_id).has("type")  and ItemDB.get_item(item_id)["type"] == "QUEST":[m
[32m+[m		[32memit_signal("item_quest_find", item_id)[m
[32m+[m	[32memit_signal("item_add", item_id)[m
[32m+[m	[32mreturn true[m
[32m+[m[41m	[m
[32m+[m[32mfunc equip(item):[m
[32m+[m	[32memit_signal("equipped_item", item)[m
[32m+[m[41m	[m
[32m+[m[32mfunc unequip(item):[m
[32m+[m	[32memit_signal("unequipped_item", item)[m
