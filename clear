[32m+[m[32mextends Area2D[m
[32m+[m
[32m+[m[32monready var gui = get_tree().get_root().get_node("World/GuiController")[m
[32m+[m
[32m+[m[32mvar tile[m
[32m+[m[32mvar tile_region[m
[32m+[m[32mvar interacted = false[m
[32m+[m[32mvar loot = ["Rifle", "Rifle"][m
[32m+[m
[32m+[m
[32m+[m[32mfunc _ready():[m
[32m+[m	[32mset_process_unhandled_input(false)[m
[32m+[m	[32mset_process(false)[m
[32m+[m[41m	[m
[32m+[m[32mfunc _unhandled_input(event):[m
[32m+[m	[32mif Input.is_action_pressed("Interact") and !interacted:[m
[32m+[m		[32mset_process(true)[m
[32m+[m
[32m+[m	[32melif Input.is_action_just_released("Interact"):[m
[32m+[m		[32mset_process(false)[m
[32m+[m		[32m$Icon.value = 0[m
[32m+[m[41m		[m
[32m+[m[32mfunc _process(delta):[m
[32m+[m	[32minteract(delta)[m
[32m+[m[41m		[m
[32m+[m[32mfunc interact(delta) -> void:[m
[32m+[m	[32m$Icon.value += 30 * delta[m
[32m+[m
[32m+[m	[32mif $Icon.value == $Icon.max_value:[m
[32m+[m		[32mset_process(false)[m
[32m+[m		[32minteracted = true[m
[32m+[m		[32mprint("mostrando loot")[m
[32m+[m		[32mgui.show_loot(self)[m
[32m+[m		[32m$Icon.hide()[m
[32m+[m[41m		[m
[32m+[m[32mfunc show_loot():[m
[32m+[m	[32mpass[m
[32m+[m
[32m+[m
[32m+[m[32mfunc _on_LootArea_body_entered(body):[m
[32m+[m	[32mif body.is_in_group("Party"):[m
[32m+[m		[32mset_process_unhandled_input(true)[m
[32m+[m		[32m$Icon.show()[m
[32m+[m
[32m+[m
[32m+[m[32mfunc _on_LootArea_body_exited(body):[m
[32m+[m	[32mif body.is_in_group("Party"):[m
[32m+[m		[32m$Icon.hide()[m
[32m+[m		[32minteracted = false[m
[32m+[m		[32mset_process_unhandled_input(false)[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m#[m
