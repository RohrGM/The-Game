[32m+[m[32mextends Panel[m
[32m+[m
[32m+[m[32monready var slots = get_children()[m
[32m+[m[32mvar items = {}[m
[32m+[m[41m [m
[32m+[m[32mfunc _ready():[m
[32m+[m	[32mfor slot in slots:[m
[32m+[m		[32mitems[slot.name] = null[m
[32m+[m[41m [m
[32m+[m[32mfunc insert_item(item):[m
[32m+[m	[32mvar item_pos = item.rect_global_position + item.rect_size / 2[m
[32m+[m	[32mvar slot = get_slot_under_pos(item_pos)[m
[32m+[m	[32mif slot == null:[m
[32m+[m		[32mreturn false[m
[32m+[m[41m   [m
[32m+[m	[32mvar item_slot = ItemDB.get_item(item.get_meta("id"))["slot"][m
[32m+[m	[32mif item_slot != slot.name:[m
[32m+[m		[32mreturn false[m
[32m+[m	[32mif items[item_slot] != null:[m
[32m+[m		[32mreturn false[m
[32m+[m	[32mitems[item_slot] = item[m
[32m+[m	[32mitem.rect_global_position = slot.rect_global_position + slot.rect_size / 2 - item.rect_size / 2[m
[32m+[m	[32mget_parent().equip(item)[m
[32m+[m	[32mreturn true[m
[32m+[m[41m [m
[32m+[m[32mfunc grab_item(pos):[m
[32m+[m	[32mvar item = get_item_under_pos(pos)[m
[32m+[m	[32mif item == null:[m
[32m+[m		[32mreturn null[m
[32m+[m[41m   [m
[32m+[m	[32mvar item_slot = ItemDB.get_item(item.get_meta("id"))["slot"][m
[32m+[m	[32mitems[item_slot] = null[m
[32m+[m	[32mget_parent().unequip(item)[m
[32m+[m	[32mreturn item[m
[32m+[m[41m [m
[32m+[m[32mfunc get_slot_under_pos(pos):[m
[32m+[m	[32mreturn get_thing_under_pos(slots, pos)[m
[32m+[m[41m [m
[32m+[m[32mfunc get_item_under_pos(pos):[m
[32m+[m	[32mreturn get_thing_under_pos(items.values(), pos)[m
[32m+[m[41m [m
[32m+[m[32mfunc get_thing_under_pos(arr, pos):[m
[32m+[m	[32mfor thing in arr:[m
[32m+[m		[32mif thing != null and thing.get_global_rect().has_point(pos):[m
[32m+[m			[32mreturn thing[m
[32m+[m	[32mreturn null[m
[32m+[m
