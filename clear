[32m+[m
[32m+[m[32mvar attributes = {[m
[32m+[m	[32m"str": 5,[m
[32m+[m	[32m"const": 5,[m
[32m+[m	[32m"dex": 5,[m
[32m+[m	[32m"action_points": 5,[m
[32m+[m	[32m"ca": 10[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvar weapon = {[m
[32m+[m	[32m"damage": Vector2(1,4),[m
[32m+[m	[32m"distance": 1,[m
[32m+[m	[32m"cost": 1[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunc move_at(pos, anim , speed = 20) -> void:[m
[32m+[m	[32mif anim == "Run":[m
[32m+[m		[32mspeed *= 1.5[m
[32m+[m	[32mstop_move()[m
[32m+[m	[32mmv = m.instance()[m
[32m+[m	[32madd_child(mv)[m
[32m+[m	[32mmv.start(get_tree().get_root().get_node("World").get_closest_point(pos), speed, anim)[m
[32m+[m	[32mmv.connect("in_position", self, "_on_MoveAt_in_position")[m
[32m+[m[41m	[m
[32m+[m[32mfunc get_ca():[m
[32m+[m	[32mreturn attributes.ca[m
[32m+[m
[32m+[m[32mfunc stop_move():[m
[32m+[m	[32mif is_instance_valid(mv):[m
[32m+[m		[32mtravel_anim("Idle")[m
[32m+[m		[32mmv.queue_free()[m
[32m+[m[41m		[m
[32m+[m[32mfunc roll_dice(value : Vector2):[m
[32m+[m	[32mvar result = 0[m
[32m+[m	[32mvar rng = RandomNumberGenerator.new()[m
[32m+[m	[32mrng.randomize()[m
[32m+[m	[32mfor i in value.x:[m
[32m+[m		[32mresult += rng.randi_range(1, value.y)[m
[32m+[m	[32mreturn result[m
[32m+[m[41m	[m
[32m+[m[41m		[m
[32m+[m[32mfunc travel_anim(anim : String) -> void:[m
[32m+[m	[32m$AnimationTree.get("parameters/playback").travel(anim)[m
[32m+[m
[32m+[m[32mfunc update_anim_tree(vector : Vector2) -> void:[m
[32m+[m	[32m$AnimationTree.set("parameters/Idle/blend_position", vector.x)[m
[32m+[m	[32m$AnimationTree.set("parameters/Run/blend_position", vector.x)[m
[32m+[m	[32m$AnimationTree.set("parameters/BtIdle/blend_position", vector.x)[m
[32m+[m	[32m$AnimationTree.set("parameters/Punch/blend_position", vector.x)[m
[32m+[m	[32m$AnimationTree.set("parameters/Hit/blend_position", vector.x)[m
[32m+[m
[32m+[m[32mfunc center_block(pos):[m
[32m+[m	[32mvar new_pos = Vector2(int(pos.x / 16), int(pos.y / 16))[m
[32m+[m	[32mnew_pos = Vector2(new_pos.x * 16 + 8, new_pos.y * 16 + 7)[m
[32m+[m[41m	[m
[32m+[m	[32mreturn new_pos[m
