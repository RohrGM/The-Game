[32m+[m[32mextends Node2D[m
[32m+[m
[32m+[m[32menum States { IDLE, FOLLOW }[m
[32m+[m
[32m+[m[32mconst MASS = 1.0[m
[32m+[m[32mconst ARRIVE_DISTANCE = 2.0[m
[32m+[m
[32m+[m[32mexport(float) var speed = 100.0[m
[32m+[m[32mvar _state = null[m
[32m+[m
[32m+[m[32mvar _path = [][m
[32m+[m[32mvar _target_point_world = Vector2()[m
[32m+[m[32mvar _target_position = Vector2()[m
[32m+[m
[32m+[m[32mvar _velocity = Vector2()[m
[32m+[m
[32m+[m[32mfunc _ready():[m
[32m+[m	[32m_change_state(States.IDLE)[m
[32m+[m
[32m+[m
[32m+[m[32mfunc _process(_delta):[m
[32m+[m	[32mif _state != States.FOLLOW:[m
[32m+[m		[32mreturn[m
[32m+[m	[32mvar _arrived_to_next_point = _move_to(_target_point_world)[m
[32m+[m	[32mif _arrived_to_next_point:[m
[32m+[m		[32m_path.remove(0)[m
[32m+[m		[32mif len(_path) == 0:[m
[32m+[m			[32m_change_state(States.IDLE)[m
[32m+[m			[32mreturn[m
[32m+[m		[32m_target_point_world = _path[0][m
[32m+[m
[32m+[m
[32m+[m[32mfunc _unhandled_input(event):[m
[32m+[m	[32mif event.is_action_pressed("left_click"):[m
[32m+[m		[32mvar global_mouse_pos = get_global_mouse_position()[m
[32m+[m		[32mif Input.is_key_pressed(KEY_SHIFT):[m
[32m+[m			[32mglobal_position = global_mouse_pos[m
[32m+[m		[32melse:[m
[32m+[m			[32m_target_position = global_mouse_pos[m
[32m+[m		[32m_change_state(States.FOLLOW)[m
[32m+[m
[32m+[m
[32m+[m[32mfunc _move_to(world_position):[m
[32m+[m	[32mvar desired_velocity = (world_position - position).normalized() * speed[m
[32m+[m	[32mvar steering = desired_velocity - _velocity[m
[32m+[m	[32m_velocity += steering / MASS[m
[32m+[m	[32mposition += _velocity * get_process_delta_time()[m
[32m+[m	[32mreturn position.distance_to(world_position) < ARRIVE_DISTANCE[m
[32m+[m
[32m+[m
[32m+[m[32mfunc _change_state(new_state):[m
[32m+[m	[32mif new_state == States.FOLLOW:[m
[32m+[m		[32m_path = get_parent().get_node("BattleMap").get_astar_path(position, _target_position)[m
[32m+[m		[32mif not _path or len(_path) == 1:[m
[32m+[m			[32m_change_state(States.IDLE)[m
[32m+[m			[32mreturn[m[41m [m
[32m+[m		[32m# The index 0 is the starting cell.[m
[32m+[m		[32m# We don't want the character to move back to it in this example.[m
[32m+[m		[32m_target_point_world = _path[1][m
[32m+[m	[32m_state = new_state[m
[1mdiff --git a/Scripts/SaveGame.gd b/Scripts/SaveGame.gd[m
[1mnew file mode 100644[m
[1mindex 0000000..5f2528c[m
[1m--- /dev/null[m
[1m+++ b/Scripts/SaveGame.gd[m
[36m@@ -0,0 +1,38 @@[m
[32m+[m[32mextends Node2D[m
[32m+[m
[32m+[m[32mconst save_path = "user://save.dat"[m
[32m+[m[32mvar party_save = [][m
[32m+[m
[32m+[m[32mfunc _ready():[m
[32m+[m	[32mload_game()[m
[32m+[m[41m	[m
[32m+[m[32mfunc _unhandled_input(event):[m
[32m+[m	[32mif event.is_action_pressed("save"):[m
[32m+[m		[32msave_game()[m
[32m+[m
[32m+[m[32mfunc save_game():[m
[32m+[m	[32mvar party = get_tree().get_nodes_in_group("Party")[m
[32m+[m	[32mfor i in party:[m
[32m+[m		[32mvar p = {[m
[32m+[m			[32m"pos" : i.position[m
[32m+[m		[32m}[m
[32m+[m		[32mparty_save.append(p)[m
[32m+[m[41m		[m
[32m+[m	[32mvar file = File.new()[m
[32m+[m	[32mvar error = file.open(save_path, File.WRITE)[m
[32m+[m	[32mif error == OK:[m
[32m+[m		[32mfile.store_var(party_save)[m
[32m+[m		[32mfile.close()[m
[32m+[m		[32mprint("salvo")[m
[32m+[m[41m		[m
[32m+[m[32mfunc load_game():[m
[32m+[m	[32mvar file = File.new()[m
[32m+[m	[32mif file.file_exists(save_path):[m
[32m+[m		[32mvar error = file.open(save_path, File.READ)[m
[32m+[m		[32mif error == OK:[m
[32m+[m			[32mvar data = file.get_var()[m
[32m+[m			[32mprint("carregado")[m
[32m+[m			[32mfile.close()[m
[32m+[m			[32mvar party = get_tree().get_nodes_in_group("Party")[m
[32m+[m			[32mfor i in range(party.size()):[m
[32m+[m				[32mparty[i].position = data[i].pos[m
[1mdiff --git a/Scripts/Train.gd b/Scripts/Train.gd[m
[1mnew file mode 100644[m
[1mindex 0000000..47f3b0d[m
[1m--- /dev/null[m
[1m+++ b/Scripts/Train.gd[m
[36m@@ -0,0 +1,20 @@[m
[32m+[m[32mextends KinematicBody2D[m
[32m+[m
[32m+[m[32mfunc _ready():[m
[32m+[m	[32mset_process_unhandled_input(false)[m
[32m+[m[41m	[m
[32m+[m[32mfunc _unhandled_input(event):[m
[32m+[m	[32mif event.is_action_pressed("interact"):[m
[32m+[m		[32mget_tree().get_root().get_node("World/Save").save_game()[m
[32m+[m[32m# warning-ignore:return_value_discarded[m
[32m+[m		[32mget_tree().change_scene("res://Scenes/Map.tscn")[m
[32m+[m[41m		[m
[32m+[m[41m	[m
[32m+[m[32m# warning-ignore:unused_argument[m
[32m+[m[32mfunc _on_Area2D_body_entered(body):[m
[32m+[m	[32mset_process_unhandled_input(true)[m
