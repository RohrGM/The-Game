[32m+[m[32mfunc take_damage(value):[m
[32m+[m	[32mlife -= value[m
[32m+[m	[32mvar message = bt_message.instance()[m
[32m+[m	[32madd_child(message)[m
[32m+[m	[32mmessage.rect_global_position = global_position + Vector2(-46, -20)[m
[32m+[m	[32mmessage.start(String(value))[m
[32m+[m	[32mif life <= 0:[m
[32m+[m		[32mlife = 0[m
[32m+[m		[32mdead()[m
[32m+[m[41m		[m
[32m+[m[32mfunc dead():[m
[32m+[m	[32mbattle.clear_deads()[m
[32m+[m	[32mqueue_free()[m
[32m+[m[41m	[m
[32m+[m[32m #TURN FUNCTIONS###################################################################################[m
[32m+[m
[32m+[m
[32m+[m[32mfunc pos_in_range(pos, distance):[m
[32m+[m	[32mvar nav = get_tree().get_root().get_node("World")[m
[32m+[m	[32mvar path = nav.get_simple_path(position, pos, false)[m
[32m+[m	[32mpath.remove(0)[m
[32m+[m[41m	[m
[32m+[m	[32mif path.size() -1 <= distance:[m
[32m+[m		[32mreturn path.size()-1[m
[32m+[m	[32mreturn 0[m
[32m+[m[41m	[m
[32m+[m[32mfunc move_in_battle(p):[m
[32m+[m	[32mvar cells_range = [][m
[32m+[m	[32mvar in_range = Vector2()[m
[32m+[m	[32mvar distance = .0[m
[32m+[m[41m	[m
[32m+[m	[32mvar pos = position - Vector2(attributes.action_points, attributes.action_points) * 16[m
[32m+[m	[32mfor i in (attributes.action_points * 2 + 1):[m
[32m+[m		[32mfor j in (attributes.action_points * 2 + 1):[m
[32m+[m			[32mif pos_in_range(pos + Vector2(i * 16, j * 16), attributes.action_points) > 0:[m
[32m+[m				[32mvar cell = map.world_to_map(pos + Vector2(i * 16, j * 16))[m
[32m+[m				[32mif not cell in battle.get_busy_cells():[m
[32m+[m					[32mcells_range.append(cell)[m
[32m+[m[41m					[m
[32m+[m	[32mif cells_range.size() > 0:[m
[32m+[m		[32min_range = cells_range[0][m
[32m+[m		[32mdistance = map.map_to_world(in_range).distance_to(p)[m
[32m+[m[41m		[m
[32m+[m		[32mfor i in cells_range:[m
[32m+[m			[32mvar d = map.map_to_world(i).distance_to(p)[m
[32m+[m			[32mif distance > d:[m
[32m+[m				[32mdistance = d[m
[32m+[m				[32min_range = i[m
[32m+[m		[32mreturn map.map_to_world(in_range)[m
[32m+[m	[32mreturn null[m
[32m+[m[41m	[m
[32m+[m[32mfunc attack_range(p):[m
[32m+[m	[32mvar cells_range = [][m
[32m+[m	[32mp = map.world_to_map(p)[m
[32m+[m	[32mvar pos = position - Vector2(weapon.distance, weapon.distance) * 16[m
[32m+[m	[32mvar my_pos = map.world_to_map(position)[m
[32m+[m[41m	[m
[32m+[m	[32mfor i in (weapon.distance * 2 + 1):[m
[32m+[m		[32mfor j in (weapon.distance * 2 + 1):[m
[32m+[m			[32mif pos_in_range(pos + Vector2(i * 16, j * 16), weapon.distance) > 0:[m
[32m+[m				[32mvar cell = map.world_to_map(pos + Vector2(i * 16, j * 16))[m
[32m+[m				[32mif cell.x == my_pos.x or cell.y == my_pos.y:[m
[32m+[m					[32mcells_range.append(cell)[m
[32m+[m	[32mif p in cells_range:[m
[32m+[m		[32mreturn true[m
[32m+[m	[32mreturn false[m
[32m+[m[41m	[m
[32m+[m[32mfunc in_cell(cell):[m
[32m+[m	[32mif map.world_to_map(position) == cell:[m
[32m+[m		[32mreturn true[m
[32m+[m	[32mreturn false[m
[32m+[m
[32m+[m[32mfunc get_enemy():[m
[32m+[m	[32mif target == null:[m
[32m+[m		[32mtarget = get_tree().get_nodes_in_group("Party")[0][m
[32m+[m	[32mreturn target[m
[32m+[m[41m	[m
[32m+[m[32mfunc attack():[m
[32m+[m	[32mif attributes.action_points >= weapon.cost:[m
[32m+[m		[32mattributes.action_points -= weapon.cost[m
[32m+[m		[32mtravel_anim("Attack")[m
[32m+[m	[32melse:[m
[32m+[m		[32mbattle.next_turn()[m
[32m+[m[41m		[m
[32m+[m[32mfunc attack_test():[m
[32m+[m	[32mif roll_dice(Vector2(1, 20)) > get_enemy().get_ca():[m
[32m+[m		[32mget_enemy().take_damage(roll_dice(weapon.damage), self)[m
[32m+[m	[32melse:[m
[32m+[m		[32mvar message = bt_message.instance()[m
[32m+[m		[32madd_child(message)[m
[32m+[m		[32mmessage.rect_global_position = global_position + Vector2(-46, -20)[m
[32m+[m		[32mmessage.start("MISS")[m[41m           [m
