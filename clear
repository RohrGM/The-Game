[32m+[m[32mextends Base[m
[32m+[m
[32m+[m[32mclass_name PlayerBase[m
[32m+[m
[32m+[m[32mvar fallow = null[m
[32m+[m[32mvar life = 10[m
[32m+[m[32mvar battle_icon = ""[m
[32m+[m[32mvar cell_hover = Vector2.ZERO[m
[32m+[m[32mvar cells_range = [][m
[32m+[m[32mvar current_target = null[m
[32m+[m
[32m+[m[32monready var gui = get_tree().get_root().get_node("World/GuiController")[m
[32m+[m
[32m+[m[32menum mode{[m
[32m+[m	[32mBLOCKED,[m
[32m+[m	[32mFREE,[m
[32m+[m	[32mTURN,[m
[32m+[m	[32mFALLOWER[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvar status = {[m
[32m+[m	[32m"bleeding": 0,[m
[32m+[m	[32m"poisoned": 0,[m
[32m+[m	[32m"hungry": false[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32menum skills{[m
[32m+[m	[32mMOVE,[m
[32m+[m	[32mPUNCH[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvar current_mode = mode.FREE[m
[32m+[m
[32m+[m[32m# warning-ignore:unused_argument[m
[32m+[m[32mfunc _physics_process(delta):[m
[32m+[m[41m	[m
[32m+[m	[32mif fallow != null:[m
[32m+[m		[32mif position.distance_to(fallow.position) > 40:[m
[32m+[m			[32mrandomize()[m
[32m+[m			[32mmove_at(fallow.position + Vector2(int(rand_range(5, 15)),int(rand_range(5, 15))), "Run")[m
[32m+[m
[32m+[m[32mfunc _unhandled_input(event):[m
[32m+[m[41m	[m
[32m+[m	[32mmatch current_mode:[m
[32m+[m		[32mmode.FREE:[m
[32m+[m			[32mif event.is_action_pressed("left_click"):[m
[32m+[m				[32mmove_at(get_global_mouse_position(), "Run", 25)[m
[32m+[m				[32mgui.hide_windows()[m
[32m+[m		[32mmode.TURN:[m
[32m+[m			[32mvar pos = map.world_to_map(get_global_mouse_position())[m
[32m+[m			[32mif pos in cells_range:[m
[32m+[m				[32mif pos != cell_hover:[m
[32m+[m					[32mif map.get_cellv(pos) == 0:[m
[32m+[m						[32mif cell_hover in cells_range:[m
[32m+[m							[32mmap.set_cellv(cell_hover, 0)[m
[32m+[m						[32melse:[m
[32m+[m							[32mmap.set_cellv(cell_hover, -1)[m
[32m+[m						[32mcell_hover = pos[m
[32m+[m						[32mmap.set_cellv(cell_hover, 1)[m
[32m+[m					[32melif map.get_cellv(pos) == 3:[m
[32m+[m						[32mif cell_hover in cells_range:[m
[32m+[m							[32mmap.set_cellv(cell_hover, 3)[m
[32m+[m						[32melse:[m
[32m+[m							[32mmap.set_cellv(cell_hover, -1)[m
[32m+[m						[32mcell_hover = pos[m
[32m+[m						[32mmap.set_cellv(cell_hover, 2)[m
[32m+[m[41m						[m
[32m+[m			[32mif event.is_action_pressed("left_click"):[m
[32m+[m				[32mif map.get_cellv(cell_hover) == 2:[m
[32m+[m					[32mattack(cell_hover)[m
[32m+[m					[32mclear_cells()[m
[32m+[m				[32melif map.get_cellv(cell_hover) == 1:[m
[32m+[m					[32mvar new_pos = center_block(map.map_to_world(cell_hover))[m
[32m+[m					[32mattributes.action_points -= pos_in_range(new_pos, attributes.action_points)[m
[32m+[m					[32mmove_at(center_block(new_pos), "Run")[m
[32m+[m					[32mclear_cells()[m
[32m+[m[41m		[m
[32m+[m[32mfunc take_damage(value, enemy):[m
[32m+[m	[32mtravel_anim("Hit")[m
[32m+[m	[32mlife -= value[m
[32m+[m[41m	[m
[32m+[m	[32mvar direction = Vector2(enemy.position.x - position.x, 0)[m
[32m+[m	[32mupdate_anim_tree(direction)[m
[32m+[m[41m	[m
[32m+[m	[32mvar message = bt_message.instance()[m
[32m+[m	[32madd_child(message)[m
[32m+[m	[32mmessage.rect_global_position = global_position + Vector2(-46, -20)[m
[32m+[m	[32mmessage.start(String(value))[m
[32m+[m	[32mif life <= 0:[m
[32m+[m		[32mlife = 0[m
[32m+[m		[32mdead()[m
[32m+[m[41m		[m
[32m+[m[32mfunc dead():[m
[32m+[m	[32mprint("morri")[m
[32m+[m
[32m+[m[32mfunc _on_MoveAt_in_position():[m
[32m+[m	[32mif current_mode != mode.FREE and current_mode != mode.FALLOWER:[m
[32m+[m		[32mtravel_anim("BtIdle")[m
[32m+[m	[32melse:[m
[32m+[m		[32mtravel_anim("Idle")[m
[32m+[m	[32mmatch current_mode:[m
[32m+[m		[32mmode.TURN:[m
[32m+[m			[32mif attributes.action_points > 0:[m
[32m+[m				[32mshow_move_range()[m
[32m+[m			[32melse:[m
[32m+[m				[32mbattle.next_turn()[m
[32m+[m[41m			[m
[32m+[m
[32m+[m[32m #TURN FUNCTIONS###################################################################################[m
[32m+[m
[32m+[m[32mfunc in_cell(cell):[m
[32m+[m	[32mif map.world_to_map(position) == cell:[m
[32m+[m		[32mreturn true[m
[32m+[m	[32mreturn false[m
[32m+[m
[32m+[m[32mfunc pos_in_range(pos, distance):[m
[32m+[m	[32mvar nav = get_tree().get_root().get_node("World")[m
[32m+[m	[32mvar path = nav.get_simple_path(position, pos, false)[m
[32m+[m	[32mpath.remove(0)[m
[32m+[m[41m	[m
[32m+[m	[32mif path.size() -1 <= distance:[m
[32m+[m		[32mreturn path.size()-1[m
[32m+[m	[32mreturn 0[m
[32m+[m[41m	[m
[32m+[m[32mfunc clear_cells():[m
[32m+[m	[32mfor i in cells_range:[m
[32m+[m		[32mmap.set_cellv(i, -1)[m
[32m+[m[41m		[m
[32m+[m	[32mcells_range = [][m
[32m+[m[41m	[m
[32m+[m[32mfunc show_move_range():[m
[32m+[m	[32mclear_cells()[m
[32m+[m	[32mvar pos = position - Vector2(attributes.action_points, attributes.action_points) * 16[m
[32m+[m	[32mfor i in (attributes.action_points * 2 + 1):[m
[32m+[m		[32mfor j in (attributes.action_points * 2 + 1):[m
[32m+[m			[32mif pos_in_range(pos + Vector2(i * 16, j * 16), attributes.action_points) > 0:[m
[32m+[m				[32mvar cell = map.world_to_map(pos + Vector2(i * 16, j * 16))[m
[32m+[m				[32mif not cell in battle.get_busy_cells():[m
[32m+[m					[32mcells_range.append(cell)[m
[32m+[m					[32mmap.set_cellv(cell, 0)[m
[32m+[m[41m					[m
[32m+[m[32mfunc show_attack_range():[m
[32m+[m	[32mclear_cells()[m
[32m+[m	[32mvar pos = position - Vector2(weapon.distance, weapon.distance) * 16[m
[32m+[m	[32mvar my_pos = map.world_to_map(position)[m
[32m+[m[41m	[m
[32m+[m	[32mfor i in (weapon.distance * 2 + 1):[m
[32m+[m		[32mfor j in (weapon.distance * 2 + 1):[m
[32m+[m[41m			[m
[32m+[m			[32mif pos_in_range(pos + Vector2(i * 16, j * 16), weapon.distance) > 0:[m
[32m+[m				[32mvar cell = map.world_to_map(pos + Vector2(i * 16, j * 16))[m
[32m+[m				[32mif cell.x == my_pos.x or cell.y == my_pos.y:[m
[32m+[m					[32mcells_range.append(cell)[m
[32m+[m					[32mmap.set_cellv(cell, 3)[m
[32m+[m[41m	[m
[32m+[m[41m	[m
[32m+[m[32mfunc show_skill(sk):[m
[32m+[m	[32mmatch sk:[m
[32m+[m		[32m"MOVE":[m
[32m+[m			[32mshow_move_range()[m
[32m+[m		[32m"PUNCH":[m
[32m+[m			[32mshow_attack_range()[m
[32m+[m
[32m+[m[32mfunc set_mode(md, agentF = null):[m
[32m+[m	[32mset_physics_process(false)[m
[32m+[m[41m	[m
[32m+[m	[32mmatch current_mode:[m
[32m+[m		[32mmode.TURN:[m
[32m+[m			[32mclear_cells()[m
[32m+[m		[32mmode.BLOCKED:[m
[32m+[m			[32mclear_cells()[m
[32m+[m[41m	[m
[32m+[m	[32mcurrent_mode = md[m
[32m+[m
[32m+[m	[32mmatch current_mode:[m
[32m+[m		[32mmode.BLOCKED:[m
[32m+[m			[32mmove_at(center_block(position), "Run")[m
[32m+[m		[32mmode.FREE:[m
[32m+[m			[32m$Camera2D.current = true[m
[32m+[m			[32mstop_move()[m
[32m+[m		[32mmode.FALLOWER:[m
[32m+[m			[32mset_physics_process(true)[m
[32m+[m			[32mfallow = agentF[m
[32m+[m		[32mmode.TURN:[m
[32m+[m			[32m$Camera2D.current = true[m
[32m+[m			[32mattributes.action_points = attributes.dex[m
[32m+[m			[32mbattle.show_skills(skills, self)[m
[32m+[m
[32m+[m[32mfunc attack(cell):[m
[32m+[m	[32mcurrent_target = battle.get_agent_in_cell(cell)[m
[32m+[m	[32mif is_instance_valid(current_target) and weapon.cost <= attributes.action_points:[m
[32m+[m		[32mattributes.action_points -= weapon.cost[m
[32m+[m		[32mtravel_anim("Punch")[m
[32m+[m[41m		[m
[32m+[m[32mfunc hit():[m
[32m+[m	[32mif is_instance_valid(current_target):[m
[32m+[m		[32mcurrent_target.take_damage(roll_dice(weapon.damage))[m
[32m+[m		[32mcurrent_target = null[m
[32m+[m[41m			[m
[32m+[m[32mfunc end_attack():[m
[32m+[m	[32mif battle.active:[m
[32m+[m		[32mif attributes.action_points > 0 :[m
[32m+[m			[32mshow_attack_range()[m
[32m+[m		[32melse:[m
[32m+[m			[32mprint("proximo")[m
[32m+[m			[32mbattle.next_turn()[m
[32m+[m[41m	[m
[32m+[m[41m		[m
[32m+[m[41m		[m
[32m+[m
[32m+[m
