[32m+[m[32mextends Control[m
[32m+[m
[32m+[m[32monready var party = get_tree().get_nodes_in_group("Party")[m
[32m+[m
[32m+[m[32mfunc _ready():[m
[32m+[m	[32mfor i in get_children():[m
[32m+[m		[32mi.connect("set_agent", self, "set_agent")[m
[32m+[m	[32mset_agent(0)[m
[32m+[m[41m		[m
[32m+[m
[32m+[m[32mfunc set_agent(agent):[m
[32m+[m[41m	[m
[32m+[m	[32mfor i in range(party.size()):[m
[32m+[m		[32mif i != agent:[m
[32m+[m			[32mparty[i].set_mode(party[i].mode.FALLOWER, party[agent])[m
[32m+[m		[32melse:[m
[32m+[m			[32mparty[i].set_mode(party[i].mode.FREE)[m
