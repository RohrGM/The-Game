[32m+[m		[32mturn = 0[m[41m [m
[32m+[m	[32m$BattleParty.get_child(turn).my_turn()[m
[32m+[m[41m	[m
[32m+[m[32mfunc clear_deads():[m
[32m+[m	[32mfor i in $BattleParty.get_children():[m
[32m+[m		[32mif i.agent.life <= 0:[m
[32m+[m			[32mdeads.append(i.agent)[m
[32m+[m			[32mi.queue_free()[m
